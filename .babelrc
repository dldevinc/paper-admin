{
    "presets": [
        [
            "@babel/preset-env",
            {
                "exclude": ["transform-async-to-generator", "transform-regenerator"],
                "loose": true,
                "modules": false
            }
        ]
    ],
    "plugins": [
        "lodash",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import",
        ["module:fast-async", { "spec": true }],
        [
            "transform-imports",
            {
                "lodash": {
                    "transform": "lodash/${member}",
                    "preventFullImport": true
                }
            }
        ]
    ]
}
